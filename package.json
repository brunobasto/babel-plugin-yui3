{
  "version": "0.0.1",
  "description": "Babel plugin for YUI3",
  "name": "babel-plugin-yui3",
  "author": "Bruno Basto",
  "main": "build/index.js",
  "dependencies": {
    "babel-core": "^6.21.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-template": "^6.16.0",
    "babel-types": "^6.21.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "7.2.3",
    "eslint": "4.10.0",
    "eslint-config-liferay": "1.0.2",
    "prettier-eslint-cli": "^4.7.0",
    "babel-register": "^6.7.2",
    "mocha": "^2.2.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brunobasto/babel-plugin-yui3.git"
  },
  "scripts": {
    "format": "prettier-eslint --write 'src/**/*.js' 'features/**/*.js'",
    "lint": "eslint src/ features/",
    "build": "babel src -d build --optional es7.decorators",
    "compile": "babel features -d demos --optional es7.decorators "
  },
  "keywords": [
    "babel-plugin",
    "YUI",
    "YUI3"
  ]
}
